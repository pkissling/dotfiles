{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": ["mise"],
      "extractVersion": "^(?<version>\\d+\\.\\d+)"
    },
    {
      "matchPackageNames": ["ruby"],
      "extractVersion": "^(?<version>\\d+\\.\\d+)"
    }
  ]
}
