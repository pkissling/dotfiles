# aliases
alias argo="kubectl port-forward service/argocd-server 8910:443 -n argocd"
alias azena="mv ~/.terraform.d/plugins.azena ~/.terraform.d/plugins && mv ~/.aws/credentials ~/.aws/credentials.carvis && mv ~/.aws/credentials.azena ~/.aws/credentials"
alias carvis="mv ~/.terraform.d/plugins ~/.terraform.d/plugins.azena && mv ~/.aws/credentials ~/.aws/credentials.azena && mv ~/.aws/credentials.carvis ~/.aws/credentials"
alias db="kubectl port-forward deployment/db-forwarder 3320:23306"
alias keycloak_start="~/Documents/scripts/keycloak_start.sh"
alias keycloak_stop="~/Documents/scripts/keycloak_stop.sh"
alias iotdevicemockup_start="~/Documents/scripts/iotdevicemockup_start.sh"
alias iotdevicemockup_stop="~/Documents/scripts/iotdevicemockup_stop.sh"
alias goodmorning="mfa && up"
alias live='function _live(){ venv; ~/Documents/git/operations/miscellaneous/sast_live_acquire_access.sh $1; deactivate};_live'
alias mfa="~/Documents/git/operations/miscellaneous/sast_temporary_mfa_credentials.sh"
alias venv="source ~/Documents/git/operations/python/venv/bin/activate"
alias verify="mvn clean checkstyle:checkstyle spotbugs:check verify -DskipTests --file ~/Documents/git/integrator-portal/parent/pom.xml"
alias unlive="(aws sts get-caller-identity --profile sast-pilcrow-dev_Developer > /dev/null || ~/Documents/git/operations/miscellaneous/sast_temporary_mfa_credentials.sh) && venv && ~/Documents/git/operations/miscellaneous/sast_live_release_access.sh && deactivate"
