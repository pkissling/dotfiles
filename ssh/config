Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host diskstation
  HostName diskstation
  User patrick

Host diskstation-root
  HostName diskstation
  User root

Host pihole
  HostName pihole
  User pi

Host home-assistant
  HostName home-assistant
  User pi

Host github.com
    HostName github.com
    Port 22
    User git
    IdentityFile ~/.ssh/private.priv
