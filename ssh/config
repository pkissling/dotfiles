Include ~/.orbstack/ssh/config

Host *
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

Host diskstation
  HostName diskstation
  User patrick

Host diskstation-root
  HostName diskstation
  User root

Host home-server
  HostName home-server
  User home-server

Host github.com
    HostName github.com
    Port 22
    User git
    IdentityFile ~/.ssh/private.priv
